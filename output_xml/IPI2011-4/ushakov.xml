        <text value=".$$Дополнение множества"/>
        <formula id="id23" value="${\cal P}_\alpha$"/>
        <text value="будем обозначать, как обычно,"/>
        <formula id="id24" value="${\cal P}_\alpha^c$"/>
        <text value="."/>
