        <text value="Оценка. Состоятельность"/>
        <text value="Пусть"/>
        <formula id="id49" value="${\cal P}_0$"/>
        <command value="\specialsymbol">
          <braces>
            <text value="~---"/>
          </braces>
        </command>
        <text value="некоторое множество распределений и распределение"/>
        <formula id="id50" value="$P^\varepsilon$"/>
        <text value="ошибки"/>
        <formula id="id51" value="$\varepsilon$"/>
        <text value="принадлежит этому множеству:"/>
        <formula id="id52" value="$P^\varepsilon\in{\cal P}_0$"/>
        <text value=". Обозначим через"/>
        <formula id="id53" value="${\cal D}_0$"/>
        <text value="множество распределений (необязательно всех) случайной величины$X$, для которых модель (1) идентифицируема. Это значит, что если"/>
        <formula id="id54" value="$P_1,P_2\hm\in{\cal D}_0$"/>
        <text value=","/>
        <formula id="id55" value="$Q_1,Q_2\hm\in{\cal P}_0$"/>
        <text value=", и"/>
        <formula id="id56" value="$P_1\ast&#10;Q_1\hm=P_2\ast Q_2$"/>
        <text value=", то"/>
        <formula id="id57" value="$P_1\hm=P_2$"/>
        <text value="и"/>
        <formula id="id58" value="$Q_1\hm=Q_2$"/>
        <text value=". Если, например, распределение"/>
        <formula id="id59" value="$\varepsilon$"/>
        <text value="симметричное устойчивое с показателем"/>
        <formula id="id60" value="$\alpha$"/>
        <text value=", т."/>
        <text value="е."/>
        <formula id="id61" value="${\cal P}_0\hm={\cal U}_\alpha$"/>
        <text value=", то можно взять"/>
        <formula id="id62" value="${\cal D}_0\hm={\cal P}_\alpha^c$"/>
        <text value=". Пусть"/>
