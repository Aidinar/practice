      <section id="id80">
        <braces>
          <text value="\section{}"/>
        </braces>
        <text value="Методы построения множеств"/>
        <formula id="id81" value="$X_{\max}$"/>
        <text value="и"/>
        <formula id="id82" value="$Y_{\min}$"/>
        <subsection>
          <text value="Последовательное перечисление"/>
          <formula id="id83" value="$X_{\max}$"/>
          <text value="и"/>
          <formula id="id84" value="$Y_{\min}$"/>
          <text value="Достаточно очевиден поиск"/>
          <formula id="id85" value="$X_{\max}$"/>
          <text value="и"/>
          <formula id="id86" value="$Y_{\min}$"/>
          <text value="при заданной"/>
          <formula id="id87" value="$\mathcal{D} (\mathcal{P})$"/>
          <text value="путем последовательного построения множеств"/>
          <formula id="id88" value="$X_{\max}$"/>
          <text value="и"/>
          <formula id="id89" value="$Y_{\min}$"/>
          <text value=". Данный поиск осуществляется в два этапа. На первом этапе находятся все максимальные частые наборы"/>
          <formula id="id90" value="$X_{\max}$"/>
          <text value=", например алгоритмом Apriori"/>
          <command value="\cite">
            <braces>
              <braces>
                <text value="{}"/>
