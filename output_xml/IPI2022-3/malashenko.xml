            </braces>
          </braces>
          <braces/>
          <braces>
            <text value=");"/>
          </braces>
        </command>
        <command value="\quad">
          <braces>
            <text value="P(R^"/>
          </braces>
          <braces>
            <braces>
              <text value="{}"/>
              <text value="+"/>
            </braces>
          </braces>
          <braces/>
          <braces>
            <text value=")"/>
          </braces>
        </command>
        <command value="\cap">
          <braces>
            <text value="P(R^"/>
          </braces>
          <braces>
            <braces>
              <text value="{}"/>
